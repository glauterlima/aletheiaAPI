SET FOREIGN_KEY_CHECKS=0;

CREATE TABLE contagem (
	codigo BIGINT(20) PRIMARY KEY AUTO_INCREMENT,
	codigo_demanda BIGINT(20) NOT NULL,
	
	FOREIGN KEY (codigo_demanda) REFERENCES demanda(codigo)
	
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

INSERT INTO contagem (codigo_demanda, total_pf_bruto, total_pf_liquido, total_pf_plataforma, preco ) values (1, 48, 40.5, 25.6, 788.56);

